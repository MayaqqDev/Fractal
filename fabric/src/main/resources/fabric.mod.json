{
  "schemaVersion": 1,
  "id": "fractal",
  "version": "${version}",
  "name": "Fractal",
  "description": "Adds item subgroups.",
  "authors": [ "unascribed" ],
  "contact": {
    "repo": "https://github.com/DaFuqs/fractal"
  },
  "license": "MIT",
  "icon": "assets/fractal/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "de.dafuqs.fractal.fabric.FractalFabric"
    ],
    "emi": [
      "de.dafuqs.fractal.compat.FractalEMIPlugin"
    ],
    "rei_client": [
      "de.dafuqs.fractal.compat.FractalREIPlugin"
    ]
  },
  "mixins": [
    "fractal.mixins.json",
    "fractal-common.mixins.json"
  ],
  "depends": {
    "minecraft": ">=${minecraft_version}",
    "fabricloader": "*",
    "fabric": "*"
  },
  "custom": {
    "modmenu": {
      "badges": [ "library" ]
    }
  }
}
