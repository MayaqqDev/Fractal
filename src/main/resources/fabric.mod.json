{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [ "original: unascribed", "current maintainer: Dafuqs" ],
  "contact": {
    "repo": "https://github.com/DaFuqs/fractal"
  },
  "license": "MIT",
  "icon": "assets/fractal/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "de.dafuqs.fractal.fabric.FractalFabric"
    ],
    "emi": [
      "de.dafuqs.fractal.compat.FractalEMIPlugin"
    ],
    "rei_client": [
      "de.dafuqs.fractal.compat.FractalREIPlugin"
    ]
  },
  "mixins": [
    "fractal.mixins.json"
  ],
  "depends": {
    "minecraft": ">=${minecraft_version}",
    "fabricloader": "*",
    "fabric": "*"
  },
  "custom": {
    "modmenu": {
      "badges": [ "library" ]
    }
  }
}
